<h1>Teachers index</h1>
<br>
<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Gender</th>
      <th>Grade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teachers.each do |teacher| %>
      <tr>
        <td><%= teacher.first_name %></td>
        <td><%= teacher.last_name %></td>
        <td><%= teacher.gender %></td>
        <td><%= teacher.grade %></td>
        <td><%= link_to 'Show', teacher %></td>
    <% end %>
  </tbody>
</table>

<br>